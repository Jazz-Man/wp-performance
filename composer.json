{
    "name": "jazzman/wp-performance",
    "type": "wordpress-muplugin",
    "description": "wp-performance",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasyl Sokolyk",
            "email": "vsokolyk@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "conflict": {
        "roots/soil":"*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-intl": "*",
        "composer/installers": "^1",
        "google/recaptcha": "^1.2",
        "jazzman/autoload-interface": "^0.1.0",
        "phpmailer/phpmailer": "^6.1",
        "templateria/spamassassin": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "roots/wordpress": "^5.3",
        "wpackagist-plugin/contact-form-7": "^5.1"
    },
    "extra": {
        "installer-paths": {
            "vendor/wp/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "vendor/wp/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "vendor/wp/wp-content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "vendor/wp"
    },
    "suggest": {
        "jazzman/wp-performance-shortcode": "Better Shortcode Parser"
    },
    "autoload": {
        "psr-4": {
            "JazzMan\\Performance\\":"src"
        }
    }
}
